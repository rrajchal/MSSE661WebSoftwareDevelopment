{
    "compilerOptions": {
      "typeRoots": ["./node_modules/@types", "./types"],
      "module": "CommonJS",
      "moduleResolution": "node",
      "strict": true,
      "outDir": "./built",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}